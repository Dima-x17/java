class Task {
    public static void main(String[] args) {
        int a = 15;
        int b = 4;
        System.out.println(a / b);
        System.out.println(a % b);
    }
}

class Task2 {
    public static void main(String[] args) {
    int a = 17;
    int one = a / 10;
    int two = a % 10;
    System.out.println((one + two));
    }
}

class Task3 {
    public static void main(String[] args) {
        double a = 1.7;
        long b = Math.round(a);
        System.out.println(b);
    }
}
